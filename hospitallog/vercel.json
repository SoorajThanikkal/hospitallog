{
    "builds": [{
        "src": "hospitallog/wsgi.py",
        "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.8"
      }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "hospitallog/wsgi.py"
        }
    ]
}